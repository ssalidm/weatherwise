{
    "manifest_version": 3,
    "name": "WeatherWise Extension",
    "version": "1.0.0",
    "description": "A simple weather extension for personal use.",

    "permissions": [
        "geolocation" 
    ],

    "action": {
        "default_popup": "popup.html" 
    },

    "icons": {
        "16": "icon-16.png",
        "32": "icon-32.png",
        "48": "icon-48.png", 
        "144": "icon-144.png"
      }
}
