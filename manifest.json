{
    "manifest_version": 3,
    "name": "WeatherWise Browser Extension",
    "version": "1.0.0",
    "description": "A simple weather extension for personal use.",

    "permissions": [
        "geolocation" 
    ],

    "action": {
        "default_popup": "popup.html" 
    },

    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png", 
        "144": "icons/icon-144.png"
      }
}
